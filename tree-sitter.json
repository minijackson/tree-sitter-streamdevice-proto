{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "streamdevice_proto",
      "path": ".",
      "scope": "source.streamdevice_proto",
      "file-types": [
        "proto"
      ],
      "injection-regex": "^(streamdevice-)?proto$",
      "highlights": "queries/highlights.scm",
      "injections": "queries/injections.scm"
    }
  ],
  "metadata": {
    "links": {
      "repository": "https://github.com/minijackson/tree-sitter-streamdevice-proto"
    },
    "version": "0.1.0",
    "license": "MIT",
    "description": "tree-sitter grammar for StreamDevice's .proto files",
